  for uevent in $FILELIST; do\n    local PARTNAME\u003d${uevent##*PARTNAME\u003d}\n    for BLOCK in \"$@\"; do\n      if [ \"$BLOCK\" \u003d \"$PARTNAME\" ]; then\n        local FNAME\u003d${uevent%:*}\n        chmod 0 $FNAME\n      fi\n    done\n  done\n  return 0\n}\n\n# Root only at this point; hoping selinux is permissive\nif [ $(id -u) !\u003d 0 ] || [ \"$(getenforce)\" !\u003d \"Permissive\" ]; then\n\techo \"Root user only\" \u003e\u00262\n\texit 1\nfi\n\n# Disaster prevention\nSLOT\u003d$(getprop ro.boot.slot_suffix)\nfind_block boot$SLOT\n\ncd $HOMEDIR\n\n# Patch selinux policy -- error messages here are normal\n./magiskpolicy --live --magisk \"allow magisk * * *\"\nif [ ! -f /sbin/.init-stamp ]; then\n\t# Set up /root links to /sbin files\n\tmount | grep -qF rootfs\n\thave_rootfs\u003d$?\n\tif [ $have_rootfs -eq 0 ]; then\n\t\tmount -o rw,remount /\n\t\tmkdir /root 2\u003e/dev/null\n\t\tchmod 750 /root\n\t\tif ! ln /sbin/* /root/; then\n\t\t\techo \"Error making /sbin hardlinks\" \u003e\u00262\n\t\t\tsetenforce 1\n\t\t\texit 1\n\t\tfi\n\t\tmount -o ro,remount /\n\tfi\n\t# Create tmpfs /sbin overlay\n\t# This may crash on system-as-root with no /root directory\n\t./magisk -c\n\n\tif [ ! -f /sbin/magiskinit ] || [ ! -f /sbin/magisk ]; then\n\t\techo \"Bad /sbin mount?\" \u003e\u00262\n\t\tsetenforce 1\n\t\texit 1\n\tfi\n\n\t# Copy binaries\n\tcp magiskinit /sbin/\n\n\tif [ $have_rootfs -ne 0 ]; then\n\t\tmkdir /sbin/.magisk/mirror/system_root\n\t\tblock\u003d$(mount | grep \" / \" | cut -d\\  -f1)\n\t\t[ $block \u003d \"/dev/root\" ] \u0026\u0026 block\u003d/dev/block/dm-0\n\t\tmount -o ro $block /sbin/.magisk/mirror/system_root\n\t\tfor file in /sbin/.magisk/mirror/system_root/sbin/*; do\n\t\t\tif [ -L $file ]; then\n\t\t\t  cp -a $file /sbin/\n\t\t\telse\n\t\t\t  cp -ps $file /sbin/${file##*/}\n\t\t\tfi\n\t\tdone\n\tfi\n\n\texport PATH\u003d/sbin:$PATH\n\n\t# Finish startup calls\n\tmagisk --post-fs-data\n\tsleep 1\t\t# hack to prevent race with later service calls\n\tmagisk --service\n\tmagisk --boot-complete\n\n\ttouch /sbin/.init-stamp\nfi\n\nsetenforce 1\n\u0027\n\nmkdir -p $HOMEDIR\ncd $HOMEDIR || exit 1\n\nif ! cmp -s $SRCDIR/bin/magiskinit magiskinit; then\n\tcp $SRCDIR/bin/magiskinit ./\n\tchmod 700 magiskinit\n\n\tln -fs magiskinit magiskpolicy\n\tln -fs magiskinit magisk\nfi\n\nif ! cmp -s $SRCDIR/bin/mtk-su mtk-su; then\n\tcp $SRCDIR/bin/mtk-su ./\n\tchmod 700 mtk-su\nfi\n\n# strip \u0027:c512...\u0027 tail from selinux context\nctx\u003d$(cat /proc/$$/attr/current)\nnewctx\u003d${ctx/%:s0:*/:s0}\n\n# start SU daemon\nexport HOMEDIR\necho \"$SU_MINISCRIPT\" | ./mtk-su -Z $newctx\n","timeoutSeconds":10,"m_SIGUID":-8917530061799642841,"m_classType":"ShellScriptAction","m_constraintList":[],"m_isDisabled":false,"m_isOrCondition":false},{"blockNextAction":false,"m_nonRoot":false,"m_script":"su -mm -c mount -t ext4 -o nosuid,nodev,noexec,noatime /dev/block/mmcblk1p1 /data/media/0/sdcard\nrestorecon -rv /data/media/0/sdcard\nsu -mm -c mount -t sdcardfs -o nosuid,nodev,noexec,noatime,mask\u003d7,gid\u003d9997 /data/media/0/sdcard /mnt/runtime/write/emulated/0/sdcard","timeoutSeconds":600,"m_SIGUID":-6932862064440414035,"m_classType":"ShellScriptAction","m_constraintList":[],"m_isDisabled":false,"m_isOrCondition":false},{"m_SIGUID":-4990869214979909411,"m_classType":"EndIfAction","m_constraintList":[],"m_isDisabled":false,"m_isOrCondition":false}],"m_category":"Tasks","m_constraintList":[{"m_rooted":false,"m_SIGUID":-5440617161337570936,"m_classType":"IsRootedConstraint","m_constraintList":[],"m_isDisabled":false,"m_isOrCondition":false}],"m_description":"","m_descriptionOpen":false,"m_enabled":true,"m_excludeLog":false,"m_headingColor":0,"m_isOrCondition":false,"m_name":"Root and sdcard the best","m_triggerList":[{"fakeIcon":2131231017,"m_SIGUID":-8187386105375198070,"m_classType":"BootTrigger","m_constraintList":[],"m_isDisabled":false,"m_isOrCondition":false},{"m_screenOn":true,"fakeIcon":2131231017,"m_SIGUID":